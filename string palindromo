#include "stdafx.h"
#include <iostream>
#include <string>
#include <stdlib.h>



using namespace std;

int main ()
{
    long i,tam;
    string x, y =" ";
    
    cout << "Ingrese una cadena: ";
    cin >> x;
    tam=x.length();
    for(i=0;i<tam;i++)
        y=y+x.at(tam-i-1);
    
    if(y==x)
        cout << "El numero es palindromo"<< endl;
    else
        cout << "El numero no es palindromo"<< endl;
    
    return 0;
}
