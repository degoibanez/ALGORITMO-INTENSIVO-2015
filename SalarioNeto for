#include <iostream>
#include "stdafx.h"
#include "conio.h"
using namespace std;
void salarioNeto (int n)
{int i.c=0,sn,s;
 cout<<"Ingrese el numero de empleados: ";
 cin>>n;
 for(i=0;i<n;i++)
 {cout<<"Ingrese el salario: ";
 cin>>s;
 if(s>= 1480*2)
  {sn=s-((s*0.13)+(s*0.121))
  }else
       {cout<<"El salario neto es"<<sn;
       }
  }
}
